generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Type {
  id   Int    @id @default(autoincrement())
  name String @unique
  pokemon PokemonCard[]
}


model PokemonCard{
  id   Int    @id @default(autoincrement())
  name String @unique
  pokedexId Int @unique
  typeId Int
  type   Type   @relation(fields: [typeId], references: [id])
  lifePoints Int
  size Float?
  weight Float?
  imageUrl String?
}